{
    "ts-node": {
        "esm": true
    },
    "compilerOptions": {
        "module": "es2022",
        "esModuleInterop": true,
        "target": "es2022",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "declaration": true,
        "baseUrl": ".",
        "paths": {
            "*": [
                "node_modules/*"
            ]
        }
    },
    "include": [
        "task.ts"
    ],
    "exclude": [
        "ais-proxy/**",
        "eslint.config.js",
        "dist/**",
        "node_modules/**"
    ]
}